local Window = Fluent:CreateWindow({Title = "Vector Hub ",SubTitle = "By Ryuenz#6264",TabWidth = 130,Size = UDim2.fromOffset(550, 350),Acrylic = true,Theme = "Darker",MinimizeKey = Enum.KeyCode.F1 })
local Tabs = {page1 = Window:AddTab({ Title = "General", Icon = "rbxassetid://11446900930" }),page2 = Window:AddTab({ Title = "Settings", Icon = "rbxassetid://11446835336" }),page3 = Window:AddTab({ Title = "Stats", Icon = "rbxassetid://7040410130" }),page4 = Window:AddTab({ Title = "Teleport", Icon = "rbxassetid://6035190846"}),page5 = Window:AddTab({ Title = "Item", Icon = "rbxassetid://12499842920" }),page6 = Window:AddTab({ Title = "Mastery", Icon = "rbxassetid://11717242503" }),page7 = Window:AddTab({ Title = "DevilFruit", Icon = "rbxassetid://130882646" }),page8 = Window:AddTab({ Title = "Raid", Icon = "rbxassetid://13180179055" }),page13 = Window:AddTab({ Title = "Combat", Icon = "rbxassetid://7485051715" }),page9 = Window:AddTab({ Title = "Mirage", Icon = "rbxassetid://12598594243" }),page10 = Window:AddTab({ Title = "RaceV4", Icon = "rbxassetid://7558782590" }),page14 = Window:AddTab({ Title = "SeaEvent", Icon = "rbxassetid://7052520309" }),page15 = Window:AddTab({ Title = "Kitsune", Icon = "rbxassetid://8028233454" }),page16 = Window:AddTab({ Title = "Christmas", Icon = "rbxassetid://8234458975" }),page11 = Window:AddTab({ Title = "Shop", Icon = "rbxassetid://6031265976" }),page12 = Window:AddTab({ Title = "Misc", Icon = "rbxassetid://11447063791" }),Config = Window:AddTab({ Title = "Config", Icon = "settings" })}   

    local E = Tabs.page1:AddParagraph({Title = "Information",Content = "This is a paragraph.\nSecond line!"})
    function UpdateInfo()
        local gameTime = math.floor(workspace.DistributedGameTime + 0.5)
        local hour = math.floor(gameTime / (60^2)) % 24
        local minute = math.floor(gameTime / (60^1)) % 60
        local second = math.floor(gameTime / (60^0)) % 60
        local fps = workspace:GetRealPhysicsFPS()
        local Ping = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
        E:SetDesc("Username : " .. game.Players.LocalPlayer.Name .. "\nGameTime: Hours: " .. hour .. " Minutes: " .. minute .. " Seconds: " .. second .. "\nFPS: " .. fps .. "\nPing : "..Ping)
