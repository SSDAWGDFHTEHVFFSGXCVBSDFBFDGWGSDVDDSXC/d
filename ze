                    until not _G.AutoFactory or workspace.Map.Dressrosa.SmileFactory.Door.Transparency == 0
                        FastAttack = false
                end
            elseif workspace.Map.Dressrosa.SmileFactory.Door.Transparency == 0 then
                _G.AutoFactory = false
            end
        end)
        end
        end)
