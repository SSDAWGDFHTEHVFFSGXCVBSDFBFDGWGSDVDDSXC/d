                                end
                            end
                        end
                    elseif
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 1
                     then
                        if game.Workspace.Enemies:FindFirstChild("Jeremy") then
