				function(ep)
					if ep then
						if #HeadTitle.Text > 0 then
							callback(HeadTitle.Text)
							HeadTitle.Text = HeadTitle.Text
						end
					end
				end)
			end

			function functionitem:AddDropdown(options)
				--text, list, default, callback
