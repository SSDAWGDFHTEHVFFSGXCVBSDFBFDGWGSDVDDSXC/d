local UIStroke = Instance.new("UIStroke")
local UICorner = Instance.new("UICorner")

local ScreenGui = Instance.new("ScreenGui")
local ImageButton = Instance.new("ImageButton")
local RobloxButton = Enum.ButtonStyle.RobloxButton

ScreenGui.Parent = game.CoreGui
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- กำหนด Stroke และ Corner
UIStroke.Thickness = 1
UIStroke.Transparency = 0.5
UIStroke.Color = Color3.fromRGB(0, 0, 0)
UIStroke.Parent = ImageButton

UICorner.CornerRadius = UDim.new(0, 8)
UICorner.Parent = ImageButton

-- กำหนดตำแหน่งและขนาดของ ImageButton
ImageButton.Parent = ScreenGui
ImageButton.Position = UDim2.new(0.120833337, 0, 0.0952890813, 0)
ImageButton.Size = UDim2.new(0, 55, 0, 55)

-- กำหนดค่าอื่นๆ ของ ImageButton
ImageButton.Draggable = true
ImageButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ImageButton.BackgroundTransparency = 0
ImageButton.Image = "rbxassetid://13265033056" -- กำหนดรูปภาพให้กับ ImageButton
ImageButton.Style = RobloxButton               -- กำหนด Style ให้กับ ImageButton


-- เมื่อคลิกปุ่มใน ImageButton
ImageButton.MouseButton1Down:connect(function()
    game.CoreGui:FindFirstChild("VectorHub").Enabled = not game.CoreGui:FindFirstChild("VectorHub").Enabled
end)
